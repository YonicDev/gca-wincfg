{
  "name": "gca-wincfg",
  "version": "1.2.0",
  "main": "index.js",
  "description": "Nintendo® Wii U GameCube™ Adapter driver install script for Windows.",
  "os": [
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "install": "node-pre-gyp install —fallback-to-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YonicDev/gca-wincfg.git"
  },
  "keywords": [
    "gamecube",
    "wiiu",
    "controller",
    "adapter",
    "gamedev",
    "drivers",
    "gaming"
  ],
  "dependencies": {
    "nan": "^2.5.1",
    "node-pre-gyp": "^0.6.30",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "aws-sdk": "2.x"
  },
  "bin": {
    "gca-config": "cli.js"
  },
  "binary": {
    "module_name": "gca_wincfg",
    "module_path": "./build/Release/binding",
    "host": "https://github.com/YonicDev/gca-wincfg/releases/download/",
    "remote_path": "{version}"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "author": "YonicStudios",
  "license": "GPL-3.0",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/YonicDev/gca-wincfg/issues"
  },
  "homepage": "https://github.com/YonicDev/gca-wincfg#readme"
